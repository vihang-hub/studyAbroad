{
  "name": "@study-abroad/shared-database",
  "version": "0.1.0",
  "description": "Database abstraction layer with PostgreSQL and Supabase adapters, repository pattern, and soft delete support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:mutation": "stryker run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@study-abroad/shared-config": "file:../config",
    "@study-abroad/shared-feature-flags": "file:../feature-flags",
    "@supabase/supabase-js": "^2.89.0",
    "@types/winston": "^2.4.4",
    "pg": "^8.16.3",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/typescript-checker": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "database",
    "postgres",
    "supabase",
    "repository",
    "soft-delete",
    "abstraction"
  ],
  "license": "MIT"
}
